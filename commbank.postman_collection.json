	"item": [
		{
			"name": "getStateCode",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"  pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"The response has correct state_code\", () => {\r",
							"    const responseJson = pm.response.json();\r",
							"    pm.expect(responseJson.data[0].state_code).to.eql(\"NY\");\r",
							"    console.log(\"responseJson - length: \" + responseJson.data[0].state_code)\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
